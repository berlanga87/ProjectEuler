def is_prime(x):
    for i in range(2, x-1):
        if x % i == 0:
            return False
    else:
        return True

def prime_list(n):
				l = []
				for i in range (1,n):
								if is_prime(i):
												l.append(i)
												return l

def max_factor(n):
				max = None
				p = prime_list(n)
				for i in p:
								if n % i == 0:
								    max = i
				print max

max_factor(600851475143)



								
